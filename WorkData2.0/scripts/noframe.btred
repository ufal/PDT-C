#!btred -NTe noframe()
# -*- mode: cperl -*-
use warnings;
use strict;

sub noframe {
    return if $root == $this;

    my $sempos = $this->attr('gram/sempos') // "";
    if ('v' eq $sempos) {
        print($this->{t_lemma}, ' ',
              $sempos, ' ',
              $this->{functor},
              "\t"), FPosition()
            unless $this->{'val_frame.rf'};
    }
}
