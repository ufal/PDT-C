#!btred -NT -e onen()
# -*- mode: cperl -*-
use warnings;
use strict;

sub onen {
    return if $root == $this;

    if (my $anode = PML_T::GetALexNode($this)) {
        my $lemma = $anode->attr('m/lemma');
        if ($lemma eq 'onen') {
            print join "\t",
                       $this->{t_lemma},
                       $this->attr('gram/sempos') || '-',
                       "";
            FPosition();
        }
    }
}
